      </Query>
      <Fields>
        <Field Name="ACCOUNT">
          <DataField>ACCOUNT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LEDGER">
          <DataField>LEDGER</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="COMPANYCODE">
          <DataField>COMPANYCODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="STATUS">
          <DataField>STATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BALANCE">
          <DataField>BALANCE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ACCOUNTTITLE">
          <DataField>ACCOUNTTITLE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YEARPERIOD">
          <DataField>YEARPERIOD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ACTUAL">
          <Value>=IIF(FIELDS!STATUS.Value = "ACTUAL", FIELDS!BALANCE.Value, CDEC(0.00))</Value>
        </Field>
        <Field Name="BUDGET">
          <Value>=IIF(FIELDS!STATUS.Value = Parameters!BUDGET.Value, FIELDS!BALANCE.Value, CDEC(0.00))</Value>
        </Field>
        <Field Name="COMMITMENTS">
          <Value>=IIF(FIELDS!STATUS.Value = "COMMITMENTS", FIELDS!BALANCE.Value, CDEC(0.00))</Value>
        </Field>
        <Field Name="BUDGET_YTD">
          <Value>=IIF(FIELDS!YEARPERIOD.Value &lt;= Parameters!YEARPERIOD.Value,FIELDS!BUDGET.Value, CDEC(0.00))</Value>
        </Field>
        <Field Name="COMMITMENTS_YTD">
          <Value>=IIF(Fields!YEARPERIOD.Value &lt;= Parameters!YEARPERIOD.Value,Fields!COMMITMENTS.Value,CDEC(0.00))</Value>
        </Field>
        <Field Name="ACTUAL_INC_COM">
          <Value>=IIF(IsNothing(Fields!ACTUAL.Value) = TRUE ,0.00,Fields!ACTUAL.Value) + IIF(IsNothing(Fields!COMMITMENTS.Value) = TRUE ,0.00,Fields!COMMITMENTS.Value)</Value>
        </Field>
        <Field Name="LEFTTOSPEND">
          <Value>=Fields!BUDGET.Value - (Fields!ACTUAL.Value + Fields!COMMITMENTS.Value)</Value>
        </Field>
      </Fields>
    </DataSet>
