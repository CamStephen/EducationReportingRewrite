    <DataSet Name="Company">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <CommandText>SELECT        M_ACCOUNT.LEDGER, M_ACCOUNT.ACCOUNT, M_ACCOUNT.TITLE, M_ACCOUNT.TYPE, M_NAMEACCOUNT.PRENAME, M_NAMEACCOUNT.ADDRESSNAME, 
                         M_NAMEACCOUNT.ADDRESS1, M_NAMEACCOUNT.ADDRESS2, M_NAMEACCOUNT.ADDRESS3, M_NAMEACCOUNT.POSTTOWN, M_NAMEACCOUNT.COUNTY, 
                         M_NAMEACCOUNT.POSTCODE1, M_NAMEACCOUNT.POSTCODE2, M_NAMEACCOUNT.COUNTRY, M_NAMEACCOUNT.TELEPHONE, M_NAMEACCOUNT.FAXNO, 
                         M_NAMEACCOUNT.TAXNUMBER, M_NAMEACCOUNT.CONTACT, M_NAMEACCOUNT.SALUTATION, M_NAMEACCOUNT.EMAIL_ADDRESS, 
                         D_NAMEBANKINFO.TITLE AS BANKNAME, D_NAMEBANKINFO.SORTCODE, D_NAMEBANKINFO.BANKTYPE, D_NAMEBANKINFO.ACCOUNTTYPE, 
                         D_NAMEBANKINFO.ACCOUNTNUMBER, D_NAMEBANKINFO.USERREF, 
                         CASE WHEN D_NAMETERMS.TERMS = 'D' THEN 'working days' 
								WHEN D_NAMETERMS.TERMS = 'M' THEN 'Months' 
								WHEN D_NAMETERMS.TERMS = 'O' THEN 'day of month' 
								ELSE NULL END [TERMS],
						CASE WHEN D_NAMETERMS.TERMS = 'D' THEN D_NAMETERMS.TERMDAYS 
								WHEN D_NAMETERMS.TERMS = 'M' THEN D_NAMETERMS.TERMMONTHS 
								WHEN D_NAMETERMS.TERMS = 'O' THEN D_NAMETERMS.TERMDAYS 
								ELSE NULL END [TERMDAYS],D_ACCOUNTUSERFIELDS.USERFIELD_VALUE as CompanyRegNo,
								iban.userfield_value as IBAN,bic.USERFIELD_VALUE as BIC
								
FROM            M_ACCOUNT INNER JOIN
                         M_NAMEACCOUNT ON M_ACCOUNT.LEDGER = M_NAMEACCOUNT.LEDGER AND M_ACCOUNT.ACCOUNT = M_NAMEACCOUNT.ACCOUNT INNER JOIN
                         D_NAMEBANKINFO ON M_NAMEACCOUNT.LEDGER = D_NAMEBANKINFO.LEDGER AND M_NAMEACCOUNT.ACCOUNT = D_NAMEBANKINFO.ACCOUNT
                         LEFT OUTER JOIN D_NAMETERMS ON D_NAMETERMS.LEDGER = M_ACCOUNT.LEDGER AND D_NAMETERMS.ACCOUNT = M_ACCOUNT.ACCOUNT

left join D_ACCOUNTUSERFIELDS on m_account.ID = D_ACCOUNTUSERFIELDS.ACCOUNT_ID and d_accountuserfields.USERFIELD_ID in 
(select ID from M_USERFIELDS where TITLE = 'Entity Company Reg No') 
left join D_ACCOUNTUSERFIELDS iban on m_account.ID = iban.ACCOUNT_ID and iban.USERFIELD_ID in 
(select ID from M_USERFIELDS where TITLE = 'Entity IBAN')  
left join D_ACCOUNTUSERFIELDS bic on m_account.ID = bic.ACCOUNT_ID and bic.USERFIELD_ID in 
(select ID from M_USERFIELDS where TITLE = 'Entity BIC')  

WHERE                      (M_ACCOUNT.LEDGER = 'ACADEMY') AND (M_ACCOUNT.ACCOUNT = 'COMPANY')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
