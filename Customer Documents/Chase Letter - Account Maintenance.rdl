    <DataSet Name="Paylist">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ACCOUNT">
            <Value>=Parameters!ACCOUNT.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LEDGER">
            <Value>=Parameters!LEDGER.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT D_DETAILSTL.ACCCURRENCY,D_DETAILSTL.ACCOUNT, ISNULL(D_DETAILSTL.DUEDATE,D_DETAILSTL.DOCDATE) AS DOCDATE, D_DETAILSTL.DOCTYPE, D_DETAILSTL.DOCNUM,
 D_DETAILSTL.DESCRIPTION, CASE WHEN D_DETAILSTL.ACCCURRENCY = '0.0000' THEN D_DETAILSTL.VALUE ELSE D_DETAILSTL.ACCCURRENCY END AS VALUE, ma1.ledger as SMTLedger,ma1.account as STMAccount,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.PRENAME else ma1.PRENAME end as PRENAME, 
case when ma1.ACCOUNT IS NULL  then M_NAMEACCOUNT.ADDRESSNAME else ma1.ADDRESSNAME end as ADDRESSNAME,
case when ma1.ACCOUNT is  null then M_NAMEACCOUNT.ADDRESS1 else ma1.ADDRESS1 end as ADDRESS1,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.ADDRESS2 else ma1.ADDRESS2 end as ADDRESS2,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.ADDRESS3 else ma1.ADDRESS3 end as ADDRESS3,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.POSTTOWN else ma1.POSTTOWN end as POSTTOWN,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.COUNTY else ma1.COUNTY end as COUNTY,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.POSTCODE1 else ma1. POSTCODE1 end as POSTCODE1,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.POSTCODE2 else ma1.POSTCODE2 end as POSTCODE2, 
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.COUNTRY else ma1.COUNTRY end as COUNTRY,
case when ma1.TAXNUMBER is null then M_NAMEACCOUNT.TAXNUMBER else ma1.TAXNUMBER end as VatNo,
 D_DETAILSTL.USERFIELD1, D_DETAILSTL.USERFIELD2, D_DETAILSTL.USERFIELD3, getdate() as MediaDate, 
D_DETAILSTL.NOMINAL, d_detailstl.COMPANYCODE, M_NAMEACCOUNT.LEDGER, D_DETAILSTL.PAYLIST, NULL as CostCentre,
NULL as CompName,M_ACCOUNT.CURRENCY AS CURRENCY,duf.USERFIELD_VALUE as LetterLevel,case when duf.USERFIELD_VALUE &gt;= 1 then 'Yes' else 'No' end as PreviousLetterSent,
NOM.NOMAN1 AS ENTITY
 FROM   dbo.D_DETAILS D_DETAILSTL 
INNER JOIN dbo.M_NAMEACCOUNT M_NAMEACCOUNT ON D_DETAILSTL.LEDGER=M_NAMEACCOUNT.LEDGER AND D_DETAILSTL.ACCOUNT=M_NAMEACCOUNT.ACCOUNT
left outer join M_NAMEACCOUNT ma1 on m_nameaccount.account = ma1.ACCOUNT and ma1.LEDGER = 'addrsmt'

INNER JOIN M_ACCOUNT ON D_DETAILSTL.LEDGER=M_ACCOUNT.LEDGER AND D_DETAILSTL.ACCOUNT=M_ACCOUNT.ACCOUNT

INNER JOIN M_DOCUMENT ON D_DETAILSTL.DOC_ID=M_DOCUMENT.ID

LEFT JOIN D_DETAIL_USERFIELDS DUF ON D_DETAILSTL.ID = DUF.DETAIL_ID AND DUF.USERFIELD_ID IN 
(SELECT ID FROM M_USERFIELDS WHERE TITLE = 'Chase Letter Level')

INNER JOIN M_NOMINAL NOM ON D_DETAILSTL.COMPANYCODE = NOM.COMPANYCODE AND D_DETAILSTL.NOMINAL = NOM.NOMINAL
  
WHERE  D_DETAILSTL.ACCOUNT=@ACCOUNT
AND D_DETAILSTL.LEDGER = @LEDGER
AND D_DETAILSTL.detailstatus &lt;&gt; 'P'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ACCCURRENCY">
          <DataField>ACCCURRENCY</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CustomerAccount">
          <Value>=Fields!ACCOUNT.Value</Value>
        </Field>
        <Field Name="ACCOUNT">
          <DataField>ACCOUNT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Client_Address">
          <Value>=Fields!PRENAME.Value+vbcrlf+
iif(len(Fields!ADDRESSNAME.Value)=0,"",Fields!ADDRESSNAME.Value+vbcrlf)+
iif(len(Fields!ADDRESS1.Value)=0,"",Fields!ADDRESS1.Value+vbcrlf)+
iif(len(Fields!ADDRESS2.Value)=0,"",Fields!ADDRESS2.Value+vbcrlf)+
iif(len(Fields!ADDRESS3.Value)=0,"",Fields!ADDRESS3.Value+vbcrlf)+
iif(len(Fields!POSTTOWN.Value)=0,"",Fields!POSTTOWN.Value+vbcrlf)+
iif(len(Fields!COUNTY.Value)=0,"",Fields!COUNTY.Value+vbcrlf)+
Fields!POSTCODE1.Value + " " +Fields!POSTCODE2.Value+vbcrlf+
Fields!COUNTRY.Value+vbcrlf</Value>
        </Field>
        <Field Name="DOCDATE">
          <DataField>DOCDATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="to30">
          <Value>=cdec(iif(Fields!DocAge.Value&lt;30,Fields!Consol_Value.Value,0))</Value>
        </Field>
        <Field Name="DOCTYPE">
          <DataField>DOCTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="to60">
          <Value>=cdec(iif(Fields!DocAge.Value&gt;30 and Fields!DocAge.Value&lt;60,Fields!Consol_Value.Value,0))</Value>
        </Field>
        <Field Name="DOCNUM">
          <DataField>DOCNUM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="to90">
          <Value>=cdec(iif(Fields!DocAge.Value&gt;60 and Fields!DocAge.Value&lt;90,Fields!Consol_Value.Value,0))</Value>
        </Field>
        <Field Name="over90">
          <Value>=cdec(iif(Fields!DocAge.Value&gt;90 ,Fields!Consol_Value.Value,0))</Value>
        </Field>
        <Field Name="VALUE">
          <DataField>VALUE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SMTLedger">
          <DataField>SMTLedger</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DocAge">
          <Value>=DateDiff("d",cdate(Fields!DOCDATE.Value),today)</Value>
        </Field>
        <Field Name="STMAccount">
          <DataField>STMAccount</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Consol_Value">
          <Value>=IIF(Fields!ACCCURRENCY.Value=0,Fields!VALUE.Value,Fields!ACCCURRENCY.Value)</Value>
        </Field>
