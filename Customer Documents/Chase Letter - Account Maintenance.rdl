    <DataSet Name="Previous_Letter_Level">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ACCOUNT">
            <Value>=Parameters!ACCOUNT.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LEDGER">
            <Value>=Parameters!LEDGER.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select ISNULL(max(duf.userfield_value),0) as PreviousLevel
from D_DETAILS det
LEFT JOIN D_DETAIL_USERFIELDS DUF ON det.ID = DUF.DETAIL_ID AND DUF.USERFIELD_ID IN 
(SELECT ID FROM M_USERFIELDS WHERE TITLE = 'Chase Letter Level')
WHERE  det.ACCOUNT=@ACCOUNT
AND det.LEDGER = @LEDGER
AND det.detailstatus &lt;&gt; 'P'</CommandText>
      </Query>
      <Fields>
        <Field Name="PreviousLevel">
          <DataField>PreviousLevel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Letter_Text_To_Use">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PREVIOUS_LETTER_LEVEL">
            <Value>=Parameters!PREVIOUS_LETTER_LEVEL.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT com.COMMTEXT,CASE WHEN @PREVIOUS_LETTER_LEVEL &lt;=2 THEN @PREVIOUS_LETTER_LEVEL+1 ELSE @PREVIOUS_LETTER_LEVEL END Letter_Level_To_Send
from M_COMMENTS com
inner join M_ACCOUNT act on act.LEDGER = 'chaseletter' 
and act.ACCOUNT = (SELECT CASE WHEN @PREVIOUS_LETTER_LEVEL &lt;=2 THEN @PREVIOUS_LETTER_LEVEL+1 ELSE @PREVIOUS_LETTER_LEVEL END)
and act.COMMID = com.COMMID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="COMMTEXT">
          <DataField>COMMTEXT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Letter_Level_To_Send">
          <DataField>Letter_Level_To_Send</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
