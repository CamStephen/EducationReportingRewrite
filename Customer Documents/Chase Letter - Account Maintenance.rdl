    <DataSet Name="Home_Currency">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <CommandText>select acu.USERFIELD_VALUE as HomeCurrency
from D_ACCOUNTUSERFIELDS acu
inner join M_USERFIELDS usf on acu.USERFIELD_ID = usf.ID and usf.TITLE = 'Parameter Text'
inner join M_ACCOUNT act on acu.ACCOUNT_ID = act.ID and act.ledger = 'parameters' and act.ACCOUNT = 'homecurrency'</CommandText>
      </Query>
      <Fields>
        <Field Name="HomeCurrency">
          <DataField>HomeCurrency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
