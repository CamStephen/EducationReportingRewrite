<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="dsInitialSQLConnection"><DataSourceReference>dsLiveReports</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>7badc41b-673c-4d41-9146-a004eed71545</rd:DataSourceID></DataSource><DataSource Name="dsselectedDatabase"><ConnectionProperties><DataProvider>SQL</DataProvider><ConnectString>="Data Source=" &amp; Parameters!ServerName.Value &amp; ";Initial Catalog=" &amp; Parameters!CATALOG.Value &amp; ";Integrated Security = SSPI"</ConnectString></ConnectionProperties><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>ea7be33a-173f-4371-9aed-4f8b2e123162</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Paylist"><Query><DataSourceName>dsselectedDatabase</DataSourceName><QueryParameters><QueryParameter Name="@ACCOUNT"><Value>=Parameters!ACCOUNT.Value</Value></QueryParameter><QueryParameter Name="@LEDGER"><Value>=Parameters!LEDGER.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT D_DETAILSTL.ACCCURRENCY,D_DETAILSTL.ACCOUNT, isnull(D_DETAILSTL.DUEDATE,D_DETAILSTL.DOCDATE) AS DOCDATE, D_DETAILSTL.DOCTYPE, D_DETAILSTL.DOCNUM,
 D_DETAILSTL.DESCRIPTION, CASE WHEN D_DETAILSTL.ACCCURRENCY = '0.0000' THEN D_DETAILSTL.VALUE ELSE D_DETAILSTL.ACCCURRENCY END AS VALUE, ma1.ledger as SMTLedger,ma1.account as STMAccount,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.PRENAME else ma1.PRENAME end as PRENAME, 
case when ma1.ACCOUNT IS NULL  then M_NAMEACCOUNT.ADDRESSNAME else ma1.ADDRESSNAME end as ADDRESSNAME,
case when ma1.ACCOUNT is  null then M_NAMEACCOUNT.ADDRESS1 else ma1.ADDRESS1 end as ADDRESS1,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.ADDRESS2 else ma1.ADDRESS2 end as ADDRESS2,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.ADDRESS3 else ma1.ADDRESS3 end as ADDRESS3,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.POSTTOWN else ma1.POSTTOWN end as POSTTOWN,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.COUNTY else ma1.COUNTY end as COUNTY,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.POSTCODE1 else ma1. POSTCODE1 end as POSTCODE1,
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.POSTCODE2 else ma1.POSTCODE2 end as POSTCODE2, 
case when ma1.ACCOUNT is null then M_NAMEACCOUNT.COUNTRY else ma1.COUNTRY end as COUNTRY,
case when ma1.TAXNUMBER is null then M_NAMEACCOUNT.TAXNUMBER else ma1.TAXNUMBER end as VatNo,
 D_DETAILSTL.USERFIELD1, D_DETAILSTL.USERFIELD2, D_DETAILSTL.USERFIELD3, getdate() as MediaDate, 
D_DETAILSTL.NOMINAL, d_detailstl.COMPANYCODE, M_NAMEACCOUNT.LEDGER, D_DETAILSTL.PAYLIST, NULL as CostCentre,
NULL as CompName,M_ACCOUNT.CURRENCY AS CURRENCY,NOM.NOMAN1 AS ENTITY,D_DETAILSTL.DUEDATE AS DUEDATE
 FROM   dbo.D_DETAILS D_DETAILSTL 
INNER JOIN dbo.M_NAMEACCOUNT M_NAMEACCOUNT ON D_DETAILSTL.LEDGER=M_NAMEACCOUNT.LEDGER AND D_DETAILSTL.ACCOUNT=M_NAMEACCOUNT.ACCOUNT
left outer join M_NAMEACCOUNT ma1 on m_nameaccount.account = ma1.ACCOUNT and ma1.LEDGER = 'addrsmt'

INNER JOIN M_ACCOUNT ON D_DETAILSTL.LEDGER=M_ACCOUNT.LEDGER AND D_DETAILSTL.ACCOUNT=M_ACCOUNT.ACCOUNT

INNER JOIN M_DOCUMENT ON D_DETAILSTL.DOC_ID=M_DOCUMENT.ID

INNER JOIN M_NOMINAL NOM ON D_DETAILSTL.COMPANYCODE = NOM.COMPANYCODE AND D_DETAILSTL.NOMINAL = NOM.NOMINAL

  
		  
  

WHERE  D_DETAILSTL.ACCOUNT=@ACCOUNT
AND D_DETAILSTL.LEDGER = @LEDGER
AND D_DETAILSTL.detailstatus &lt;&gt; 'P'</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="CURRENCY"><DataField>CURRENCY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ACCOUNT"><DataField>ACCOUNT</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DOCDATE"><DataField>DOCDATE</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="DOCTYPE"><DataField>DOCTYPE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DOCNUM"><DataField>DOCNUM</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="DESCRIPTION"><DataField>DESCRIPTION</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="VALUE"><DataField>VALUE</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="PRENAME"><DataField>PRENAME</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESSNAME"><DataField>ADDRESSNAME</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS1"><DataField>ADDRESS1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ACCCURRENCY"><DataField>ACCCURRENCY</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="ADDRESS2"><DataField>ADDRESS2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS3"><DataField>ADDRESS3</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTTOWN"><DataField>POSTTOWN</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="COUNTY"><DataField>COUNTY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTCODE1"><DataField>POSTCODE1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTCODE2"><DataField>POSTCODE2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="COUNTRY"><DataField>COUNTRY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SMTLedger"><DataField>SMTLedger</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="USERFIELD1"><DataField>USERFIELD1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="STMAccount"><DataField>STMAccount</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="USERFIELD2"><DataField>USERFIELD2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="USERFIELD3"><DataField>USERFIELD3</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="MEDIADATE"><DataField>MediaDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="NOMINAL"><DataField>NOMINAL</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="COMPANYCODE"><DataField>COMPANYCODE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LEDGER"><DataField>LEDGER</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="PAYLIST"><DataField>PAYLIST</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CustomerAccount"><Value>=Fields!ACCOUNT.Value</Value></Field><Field Name="Client_Address"><Value>=Fields!PRENAME.Value+vbcrlf+
iif(len(Fields!ADDRESSNAME.Value)=0,"",Fields!ADDRESSNAME.Value+vbcrlf)+
iif(len(Fields!ADDRESS1.Value)=0,"",Fields!ADDRESS1.Value+vbcrlf)+
iif(len(Fields!ADDRESS2.Value)=0,"",Fields!ADDRESS2.Value+vbcrlf)+
iif(len(Fields!ADDRESS3.Value)=0,"",Fields!ADDRESS3.Value+vbcrlf)+
iif(len(Fields!POSTTOWN.Value)=0,"",Fields!POSTTOWN.Value+vbcrlf)+
iif(len(Fields!COUNTY.Value)=0,"",Fields!COUNTY.Value+vbcrlf)+
Fields!POSTCODE1.Value + " " +Fields!POSTCODE2.Value+vbcrlf+
Fields!COUNTRY.Value+vbcrlf</Value></Field><Field Name="VatNo"><DataField>VatNo</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ENTITY"><DataField>ENTITY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DUEDATE"><DataField>DUEDATE</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="CostCentre"><DataField>CostCentre</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="CompName"><DataField>CompName</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="to30"><Value>=cdec(iif(Fields!DocAge.Value&lt;30,Fields!Consol_Value.Value,0))</Value></Field><Field Name="to60"><Value>=cdec(iif(Fields!DocAge.Value&gt;30 and Fields!DocAge.Value&lt;60,Fields!Consol_Value.Value,0))</Value></Field><Field Name="to90"><Value>=cdec(iif(Fields!DocAge.Value&gt;60 and Fields!DocAge.Value&lt;90,Fields!Consol_Value.Value,0))</Value></Field><Field Name="over90"><Value>=cdec(iif(Fields!DocAge.Value&gt;90 ,Fields!Consol_Value.Value,0))</Value></Field><Field Name="DocAge"><Value>=DateDiff("d",cdate(Fields!DOCDATE.Value),today)</Value></Field><Field Name="Consol_Value"><Value>=IIF(Fields!ACCCURRENCY.Value=0,Fields!VALUE.Value,Fields!ACCCURRENCY.Value)</Value></Field></Fields></DataSet><DataSet Name="Company"><Query><DataSourceName>dsselectedDatabase</DataSourceName><CommandText>SELECT        M_ACCOUNT.LEDGER, M_ACCOUNT.ACCOUNT, M_ACCOUNT.TITLE, M_ACCOUNT.TYPE, M_NAMEACCOUNT.PRENAME, M_NAMEACCOUNT.ADDRESSNAME, 
                         M_NAMEACCOUNT.ADDRESS1, M_NAMEACCOUNT.ADDRESS2, M_NAMEACCOUNT.ADDRESS3, M_NAMEACCOUNT.POSTTOWN, M_NAMEACCOUNT.COUNTY, 
                         M_NAMEACCOUNT.POSTCODE1, M_NAMEACCOUNT.POSTCODE2, M_NAMEACCOUNT.COUNTRY, M_NAMEACCOUNT.TELEPHONE, M_NAMEACCOUNT.FAXNO, 
                         M_NAMEACCOUNT.TAXNUMBER, M_NAMEACCOUNT.CONTACT, M_NAMEACCOUNT.SALUTATION, M_NAMEACCOUNT.EMAIL_ADDRESS, 
                         D_NAMEBANKINFO.TITLE AS BANKNAME, D_NAMEBANKINFO.SORTCODE, D_NAMEBANKINFO.BANKTYPE, D_NAMEBANKINFO.ACCOUNTTYPE, 
                         D_NAMEBANKINFO.ACCOUNTNUMBER, D_NAMEBANKINFO.USERREF, 
                         CASE WHEN D_NAMETERMS.TERMS = 'D' THEN 'working days' 
								WHEN D_NAMETERMS.TERMS = 'M' THEN 'Months' 
								WHEN D_NAMETERMS.TERMS = 'O' THEN 'day of month' 
								ELSE NULL END [TERMS],
						CASE WHEN D_NAMETERMS.TERMS = 'D' THEN D_NAMETERMS.TERMDAYS 
								WHEN D_NAMETERMS.TERMS = 'M' THEN D_NAMETERMS.TERMMONTHS 
								WHEN D_NAMETERMS.TERMS = 'O' THEN D_NAMETERMS.TERMDAYS 
								ELSE NULL END [TERMDAYS],D_ACCOUNTUSERFIELDS.USERFIELD_VALUE as CompanyRegNo,
								iban.userfield_value as IBAN,bic.USERFIELD_VALUE as BIC
								
FROM            M_ACCOUNT INNER JOIN
                         M_NAMEACCOUNT ON M_ACCOUNT.LEDGER = M_NAMEACCOUNT.LEDGER AND M_ACCOUNT.ACCOUNT = M_NAMEACCOUNT.ACCOUNT INNER JOIN
                         D_NAMEBANKINFO ON M_NAMEACCOUNT.LEDGER = D_NAMEBANKINFO.LEDGER AND M_NAMEACCOUNT.ACCOUNT = D_NAMEBANKINFO.ACCOUNT
                         LEFT OUTER JOIN D_NAMETERMS ON D_NAMETERMS.LEDGER = M_ACCOUNT.LEDGER AND D_NAMETERMS.ACCOUNT = M_ACCOUNT.ACCOUNT

left join D_ACCOUNTUSERFIELDS on m_account.ID = D_ACCOUNTUSERFIELDS.ACCOUNT_ID and d_accountuserfields.USERFIELD_ID in 
(select ID from M_USERFIELDS where TITLE = 'Entity Company Reg No') 
left join D_ACCOUNTUSERFIELDS iban on m_account.ID = iban.ACCOUNT_ID and iban.USERFIELD_ID in 
(select ID from M_USERFIELDS where TITLE = 'Entity IBAN')  
left join D_ACCOUNTUSERFIELDS bic on m_account.ID = bic.ACCOUNT_ID and bic.USERFIELD_ID in 
(select ID from M_USERFIELDS where TITLE = 'Entity BIC')  

WHERE                      (M_ACCOUNT.LEDGER = 'ACADEMY') AND (M_ACCOUNT.ACCOUNT = 'COMPANY')</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="LEDGER"><DataField>LEDGER</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ACCOUNT"><DataField>ACCOUNT</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TITLE"><DataField>TITLE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TYPE"><DataField>TYPE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="PRENAME"><DataField>PRENAME</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESSNAME"><DataField>ADDRESSNAME</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS1"><DataField>ADDRESS1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS2"><DataField>ADDRESS2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS3"><DataField>ADDRESS3</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTTOWN"><DataField>POSTTOWN</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="COUNTY"><DataField>COUNTY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTCODE1"><DataField>POSTCODE1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTCODE2"><DataField>POSTCODE2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="COUNTRY"><DataField>COUNTRY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TELEPHONE"><DataField>TELEPHONE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FAXNO"><DataField>FAXNO</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TAXNUMBER"><DataField>TAXNUMBER</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CONTACT"><DataField>CONTACT</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SALUTATION"><DataField>SALUTATION</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="EMAIL_ADDRESS"><DataField>EMAIL_ADDRESS</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="BANKNAME"><DataField>BANKNAME</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SORTCODE"><DataField>SORTCODE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="BANKTYPE"><DataField>BANKTYPE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ACCOUNTTYPE"><DataField>ACCOUNTTYPE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ACCOUNTNUMBER"><DataField>ACCOUNTNUMBER</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="USERREF"><DataField>USERREF</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyRegNo"><DataField>CompanyRegNo</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TERMS"><DataField>TERMS</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TERMDAYS"><DataField>TERMDAYS</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="IBAN"><DataField>IBAN</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="BIC"><DataField>BIC</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="dsGetDisclaimer"><Query><DataSourceName>dsselectedDatabase</DataSourceName><QueryParameters><QueryParameter Name="@COMPANYCODE"><Value>=Parameters!COMPANYCODE.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        M_COMMENTS.COMMTEXT
FROM            M_ACCOUNT INNER JOIN 
				M_COMMENTS ON M_ACCOUNT.COMMID = M_COMMENTS.COMMID
WHERE        (M_ACCOUNT.LEDGER = @COMPANYCODE) AND (M_ACCOUNT.ACCOUNT = @COMPANYCODE)</CommandText></Query><Fields><Field Name="COMMTEXT"><DataField>COMMTEXT</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="dsGetServerName"><Query><DataSourceName>dsInitialSQLConnection</DataSourceName><CommandText>SELECT SERVERPROPERTY('ServerName') AS ServerName</CommandText></Query><Fields><Field Name="ServerName"><DataField>ServerName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="dsGetDatabaseName"><Query><DataSourceName>dsInitialSQLConnection</DataSourceName><CommandText>SELECT DB_NAME() AS DatabaseName</CommandText></Query><Fields><Field Name="DatabaseName"><DataField>DatabaseName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Update_Comments"><Query><DataSourceName>dsselectedDatabase</DataSourceName><QueryParameters><QueryParameter Name="@LEDGER"><Value>=Parameters!LEDGER.Value</Value></QueryParameter><QueryParameter Name="@ACCOUNT"><Value>=Parameters!ACCOUNT.Value</Value></QueryParameter><QueryParameter Name="@USER"><Value>=Parameters!USER.Value</Value><rd:UserDefined>true</rd:UserDefined></QueryParameter></QueryParameters><CommandType>StoredProcedure</CommandType><CommandText>PSF_STD_Update_Statement_Comments</CommandText></Query></DataSet><DataSet Name="Home_Currency"><Query><DataSourceName>dsselectedDatabase</DataSourceName><CommandText>select acu.USERFIELD_VALUE as HomeCurrency
from D_ACCOUNTUSERFIELDS acu
inner join M_USERFIELDS usf on acu.USERFIELD_ID = usf.ID and usf.TITLE = 'Parameter Text'
inner join M_ACCOUNT act on acu.ACCOUNT_ID = act.ID and act.ledger = 'parameters' and act.ACCOUNT = 'homecurrency'</CommandText></Query><Fields><Field Name="HomeCurrency"><DataField>HomeCurrency</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Account_Currency"><Query><DataSourceName>dsselectedDatabase</DataSourceName><QueryParameters><QueryParameter Name="@LEDGER"><Value>=Parameters!LEDGER.Value</Value></QueryParameter><QueryParameter Name="@ACCOUNT"><Value>=Parameters!ACCOUNT.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT CURRENCY AS AccountCurrency FROM M_ACCOUNT
WHERE LEDGER = @LEDGER
AND ACCOUNT = @ACCOUNT</CommandText></Query><Fields><Field Name="AccountCurrency"><DataField>AccountCurrency</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="AcademyAddress"><Query><DataSourceName>dsselectedDatabase</DataSourceName><QueryParameters><QueryParameter Name="@ACCOUNTCODE"><Value>=Parameters!ACCOUNTCODE.Value</Value></QueryParameter></QueryParameters><CommandText>
				  SELECT        M_ACCOUNT.LEDGER, M_ACCOUNT.ACCOUNT, M_ACCOUNT.TITLE, M_ACCOUNT.TYPE, M_NAMEACCOUNT.PRENAME, M_NAMEACCOUNT.ADDRESSNAME,
				  M_NAMEACCOUNT.ADDRESS1, M_NAMEACCOUNT.ADDRESS2, M_NAMEACCOUNT.ADDRESS3, M_NAMEACCOUNT.POSTTOWN, M_NAMEACCOUNT.COUNTY,
				  M_NAMEACCOUNT.POSTCODE1, M_NAMEACCOUNT.POSTCODE2, M_NAMEACCOUNT.COUNTRY, M_NAMEACCOUNT.TELEPHONE, M_NAMEACCOUNT.FAXNO,
				  M_NAMEACCOUNT.TAXNUMBER, M_NAMEACCOUNT.CONTACT, M_NAMEACCOUNT.SALUTATION, M_NAMEACCOUNT.EMAIL_ADDRESS,
				  D_NAMEBANKINFO.TITLE AS BANKNAME, D_NAMEBANKINFO.SORTCODE, D_NAMEBANKINFO.BANKTYPE, D_NAMEBANKINFO.ACCOUNTTYPE,
				  D_NAMEBANKINFO.ACCOUNTNUMBER, D_NAMEBANKINFO.USERREF
				  FROM            M_ACCOUNT INNER JOIN
				  M_NAMEACCOUNT ON M_ACCOUNT.LEDGER = M_NAMEACCOUNT.LEDGER AND M_ACCOUNT.ACCOUNT = M_NAMEACCOUNT.ACCOUNT LEFT OUTER JOIN
				  D_NAMEBANKINFO ON M_NAMEACCOUNT.LEDGER = D_NAMEBANKINFO.LEDGER AND M_NAMEACCOUNT.ACCOUNT = D_NAMEBANKINFO.ACCOUNT
				  WHERE        (M_ACCOUNT.LEDGER = 'ACADEMY') AND (SUBSTRING(M_ACCOUNT.ACCOUNT,1,3) = @ACCOUNTCODE)
			  </CommandText></Query><Fields><Field Name="LEDGER"><DataField>LEDGER</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ACCOUNT"><DataField>ACCOUNT</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TITLE"><DataField>TITLE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TYPE"><DataField>TYPE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="PRENAME"><DataField>PRENAME</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESSNAME"><DataField>ADDRESSNAME</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS1"><DataField>ADDRESS1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS2"><DataField>ADDRESS2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ADDRESS3"><DataField>ADDRESS3</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTTOWN"><DataField>POSTTOWN</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="COUNTY"><DataField>COUNTY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTCODE1"><DataField>POSTCODE1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="POSTCODE2"><DataField>POSTCODE2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="COUNTRY"><DataField>COUNTRY</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TELEPHONE"><DataField>TELEPHONE</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FAXNO"><DataField>FAXNO</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TAXNUMBER"><DataField>TAXNUMBER</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CONTACT"><DataField>CONTACT</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SALUTATION"><DataField>SALUTATION</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="EMAIL_ADDRESS"><DataField>EMAIL_