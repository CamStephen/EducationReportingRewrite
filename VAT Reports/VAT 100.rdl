    <DataSet Name="REG_VAT">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@VATPeriod">
            <Value>=Parameters!VATPeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyCode">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OUTPUTTAX">
            <Value>=Parameters!OUTPUTTAX.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@INPUTTAX">
            <Value>=Parameters!INPUTTAX.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@INC_VAT">
            <Value>=Parameters!INC_VAT.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT			D_DETAILS.DOCDATE, 
			D_DETAILS.YEARCODE + '.' + 			D_DETAILS.PERIOD [PERIOD],
			D_DETAILS.DOCTYPE,
			D_DETAILS.DOCNUM,		
			CASE VAT.ACCOUNT WHEN 'VATOUTPUT' THEN -D_DETAILS.VALUE ELSE D_DETAILS.VALUE END  BALANCE,
			D_DETAILS.DETAILSTATUS,
			D_DETAILS.DESCRIPTION, D_DETAILS.USERVALUE AS [GROSS_AMOUNT], MA2.TITLE, M_LEDGER.TYPE,			
			D_DETAILS.TAXTYPE	, 
(SELECT Title  FROM [D_KEYLISTKEY] where KEYLIST = 'vatrates' and KEYLISTKEY  = D_DETAILS.TaxType) [TAX RATE]			
			
				
FROM            D_DETAILS INNER JOIN
                         M_NOMINAL ON D_DETAILS.COMPANYCODE = M_NOMINAL.COMPANYCODE AND D_DETAILS.NOMINAL = M_NOMINAL.NOMINAL INNER JOIN
                         M_COMPANY ON D_DETAILS.COMPANYCODE = M_COMPANY.COMPANYCODE LEFT OUTER JOIN
                         M_ACCOUNT ON D_DETAILS.ACCOUNT = M_ACCOUNT.ACCOUNT AND D_DETAILS.LEDGER = M_ACCOUNT.LEDGER LEFT OUTER JOIN
                         M_LEDGER ON D_DETAILS.LEDGER = M_LEDGER.LEDGER
                         LEFT OUTER JOIN M_ACCOUNT MA2 ON D_DETAILS.XACCOUNT = MA2.ACCOUNT AND  D_DETAILS.XLEDGER = MA2.LEDGER    
						 inner JOIN (SELECT ma.ACCOUNT, DAU.USERFIELD_VALUE FROM M_ACCOUNT MA INNER JOIN D_ACCOUNTUSERFIELDS DAU ON Ma.ID = dau.ACCOUNT_ID and LEDGER = 'PARAMETERS' and (MA.ACCOUNT  = 'VATOUTPUT' OR MA.ACCOUNT = 'VATINPUT')) VAT ON VAT.USERFIELD_VALUE = M_NOMINAL.NOMAN2                         
WHERE M_LEDGER.TYPE IN ('X','T') 
AND D_DETAILS.ACCOUNT=@VATPeriod
AND D_DETAILS.COMPANYCODE=@CompanyCode
AND D_DETAILS.NOMINAL IN (@OUTPUTTAX,@INPUTTAX)
AND D_DETAILS.PERIOD = 'REG'
AND 
CASE WHEN DOCTYPE NOT LIKE '%VT%' THEN '1' WHEN DOCTYPE LIKE '%VT%' THEN CASE WHEN @INC_VAT = '1' THEN '1' WHEN @INC_VAT = '0' THEN '0' END END = '1'</CommandText>
      </Query>
      <Fields>
        <Field Name="DOCDATE">
          <DataField>DOCDATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PERIOD">
          <DataField>PERIOD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCTYPE">
          <DataField>DOCTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCNUM">
          <DataField>DOCNUM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BALANCE">
          <DataField>BALANCE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DETAILSTATUS">
          <DataField>DETAILSTATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GROSS_AMOUNT">
          <DataField>GROSS_AMOUNT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TITLE">
          <DataField>TITLE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TYPE">
          <DataField>TYPE</DataField>
