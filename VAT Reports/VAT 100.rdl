    <DataSet Name="EU_Purchases">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@VATPeriod">
            <Value>=Parameters!VATPeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyCode">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncVATTransfer">
            <Value>1</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  
	d.[uservalue] BALANCE 
	,d.DOCDATE 
	,d.YEARCODE + '.' + d.PERIOD [PERIOD] 
	,d.DOCTYPE 
	,d.DOCNUM 
	,d.DETAILSTATUS 
	,d.[DESCRIPTION]
from D_DETAILS d 
	inner join D_NOMKEYS i on d.NOMINAL = i.nominal and d.COMPANYCODE = i.COMPANYCODE
where i.KEYNAME = 'VATMTD'
	and i.KEYVALUE = 'BOX9'
	and d.LEDGER IN (SELECT LEDGER FROM M_LEDGER WHERE [TYPE] in ('T','X') )
	and d.ACCOUNT = @VATPeriod
	and d.COMPANYCODE = @COMPANYCODE 
	and (right(d.DOCTYPE,2) &lt;&gt; 'VT' and @IncVATTransfer = 0 
	or 
	@IncVATTransfer = 1)
	and TRANSACTIONTYPE = 0 </CommandText>
      </Query>
      <Fields>
        <Field Name="BALANCE">
          <DataField>BALANCE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DOCDATE">
          <DataField>DOCDATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PERIOD">
          <DataField>PERIOD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCTYPE">
          <DataField>DOCTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCNUM">
          <DataField>DOCNUM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DETAILSTATUS">
          <DataField>DETAILSTATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="EU_Sales">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@VATPeriod">
            <Value>=Parameters!VATPeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyCode">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncVATTransfer">
            <Value>1</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	(d.[uservalue] * -1) BALANCE 
	,d.DOCDATE 
	,d.YEARCODE + '.' + d.PERIOD [PERIOD] 
	,d.DOCTYPE 
	,d.DOCNUM 
	,d.DETAILSTATUS 
	,d.[DESCRIPTION]
from D_DETAILS d inner join D_NOMKEYS i on d.NOMINAL = i.nominal and d.COMPANYCODE = i.COMPANYCODE
where i.KEYNAME = 'VATMTD'
	and i.KEYVALUE = 'BOX8'
	and d.LEDGER in (SELECT LEDGER FROM M_LEDGER WHERE [TYPE] in ('T','X') )
	and d.ACCOUNT = @VATPeriod
	and d.COMPANYCODE = @COMPANYCODE
	and (right(d.DOCTYPE,2) &lt;&gt; 'VT' and @IncVATTransfer = 0 
	or 
	@IncVATTransfer = 1)
	AND TRANSACTIONTYPE = 0</CommandText>
      </Query>
