    <DataSet Name="dsGetVATINPUTNominals">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LOCATION">
            <Value>=Parameters!LOCATION.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@COMPANYCODE">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DOMAINNAME">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NOMINAL, TITLE, M_NOMINAL.COMPANYCODE, [TYPE]
FROM M_NOMINAL
INNER JOIN  
(
	SELECT 
	CompanyCode ,
	AC_Start = posfrom , 
	AC_Length = posto - posfrom + 1		
	from D_NOMANALYSIS
	WHERE AnalysisCode = 2
) AnalysisCodePosition	
ON M_NOMINAL.COMPANYCODE = AnalysisCodePosition.CompanyCode
WHERE SUBSTRING(NOMINAL,AnalysisCodePosition.AC_Start,AnalysisCodePosition.AC_Length) = (SELECT USERFIELD_VALUE FROM M_ACCOUNT MA INNER JOIN D_ACCOUNTUSERFIELDS DAU ON Ma.ID = dau.ACCOUNT_ID and LEDGER = 'PARAMETERS' and ACCOUNT  = 'VATINPUT')
AND M_NOMINAL.NOMAN1 IN (@LOCATION)
AND M_NOMINAL.COMPANYCODE IN (@COMPANYCODE)
AND dbo.PSF_RoleAllowedForUser(@DOMAINNAME, M_NOMINAL.ROLE_VIEW,NULL) = 1
ORDER BY NOMINAL</CommandText>
      </Query>
      <Fields>
        <Field Name="NOMINAL">
          <DataField>NOMINAL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TITLE">
          <DataField>TITLE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="COMPANYCODE">
          <DataField>COMPANYCODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TYPE">
          <DataField>TYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LIST">
          <Value>=FIELDS!NOMINAL.Value &amp; " - " &amp; FIELDS!TITLE.Value</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Sales_VAT">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@VATPeriod">
            <Value>=Parameters!VATPeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@COMPANYCODE">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OUTPUTTAX">
            <Value>=Parameters!OUTPUTTAX.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@INC_VAT">
            <Value>=Parameters!INC_VAT.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> SELECT		D_DETAILS.DOCDATE, 
			D_DETAILS.YEARCODE + '.' + D_DETAILS.PERIOD [PERIOD],
			D_DETAILS.DOCTYPE,
			D_DETAILS.DOCNUM,		
			-D_DETAILS.VALUE AS BALANCE,
			D_DETAILS.DETAILSTATUS,
			D_DETAILS.DESCRIPTION, 
			-D_DETAILS.USERVALUE AS [GROSS_AMOUNT], 
			MA2.TITLE, 
			M_LEDGER.TYPE,
			D_DETAILS.TAXTYPE	, 
isnull((SELECT Title  FROM [D_KEYLISTKEY] where KEYLIST = 'vatrates' and KEYLISTKEY  = D_DETAILS.TaxType),'No Vat Assigned') [TAX RATE]
FROM       D_DETAILS 
		   INNER JOIN M_NOMINAL ON D_DETAILS.COMPANYCODE = M_NOMINAL.COMPANYCODE AND D_DETAILS.NOMINAL = M_NOMINAL.NOMINAL 
		   INNER JOIN M_COMPANY ON D_DETAILS.COMPANYCODE = M_COMPANY.COMPANYCODE 
		   LEFT OUTER JOIN M_ACCOUNT ON D_DETAILS.ACCOUNT = M_ACCOUNT.ACCOUNT AND D_DETAILS.LEDGER = M_ACCOUNT.LEDGER 
		   LEFT OUTER JOIN M_LEDGER ON D_DETAILS.LEDGER = M_LEDGER.LEDGER
           LEFT OUTER JOIN M_ACCOUNT MA2 ON D_DETAILS.XACCOUNT = MA2.ACCOUNT AND  D_DETAILS.XLEDGER = MA2.LEDGER    
WHERE M_LEDGER.TYPE IN ('X','T') 
AND D_DETAILS.ACCOUNT=@VATPeriod
AND D_DETAILS.COMPANYCODE=@COMPANYCODE
AND D_DETAILS.NOMINAL IN (@OUTPUTTAX)
AND 
CASE WHEN DOCTYPE NOT LIKE '%VT%' THEN '1' WHEN DOCTYPE LIKE '%VT%' THEN CASE WHEN @INC_VAT = '1' THEN '1' WHEN @INC_VAT = '0' THEN '0' END END = '1'</CommandText>
      </Query>
      <Fields>
        <Field Name="BALANCE">
          <DataField>BALANCE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DOCDATE">
          <DataField>DOCDATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PERIOD">
          <DataField>PERIOD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCTYPE">
          <DataField>DOCTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCNUM">
          <DataField>DOCNUM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DETAILSTATUS">
          <DataField>DETAILSTATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GROSS_AMOUNT">
          <DataField>GROSS_AMOUNT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TITLE">
          <DataField>TITLE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TYPE">
          <DataField>TYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TAXTYPE">
          <DataField>TAXTYPE</DataField>
