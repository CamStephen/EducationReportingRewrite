    <DataSet Name="dsGetVATOUTPUTNominals">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LOCATION">
            <Value>=Parameters!LOCATION.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@COMPANYCODE">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DOMAINNAME">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NOMINAL, TITLE, M_NOMINAL.COMPANYCODE, [TYPE]
FROM M_NOMINAL
INNER JOIN  
(
	SELECT 
	CompanyCode ,
	AC_Start = posfrom , 
	AC_Length = posto - posfrom + 1		
	from D_NOMANALYSIS
	WHERE AnalysisCode = 2
) AnalysisCodePosition	
ON M_NOMINAL.COMPANYCODE = AnalysisCodePosition.CompanyCode
WHERE SUBSTRING(NOMINAL,AnalysisCodePosition.AC_Start,AnalysisCodePosition.AC_Length) = (SELECT USERFIELD_VALUE FROM M_ACCOUNT MA INNER JOIN D_ACCOUNTUSERFIELDS DAU ON Ma.ID = dau.ACCOUNT_ID and LEDGER = 'PARAMETERS' and ACCOUNT  = 'VATOUTPUT')
AND M_NOMINAL.NOMAN1 IN (@LOCATION)
AND M_NOMINAL.COMPANYCODE IN (@COMPANYCODE)
AND dbo.PSF_RoleAllowedForUser(@DOMAINNAME, M_NOMINAL.ROLE_VIEW,NULL) = 1
ORDER BY NOMINAL</CommandText>
      </Query>
      <Fields>
        <Field Name="NOMINAL">
          <DataField>NOMINAL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TITLE">
          <DataField>TITLE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="COMPANYCODE">
          <DataField>COMPANYCODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TYPE">
          <DataField>TYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="List">
          <Value>=FIELDS!NOMINAL.Value &amp; " - " &amp; FIELDS!TITLE.Value</Value>
        </Field>
      </Fields>
    </DataSet>
