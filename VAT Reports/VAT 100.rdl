    <DataSet Name="EU_Acquistions">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@VATPeriod">
            <Value>=Parameters!VATPeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyCode">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EUOUTPUTTAX">
            <Value>=Parameters!EUOUTPUTTAX.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select -value as BALANCE,
			D_DETAILS.DOCDATE, 
			D_DETAILS.YEARCODE + '.' + 			D_DETAILS.PERIOD [PERIOD],
			D_DETAILS.DOCTYPE,
			D_DETAILS.DOCNUM,
			D_DETAILS.DETAILSTATUS,
			D_DETAILS.DESCRIPTION  
FROM            D_DETAILS INNER JOIN
                         M_NOMINAL ON D_DETAILS.COMPANYCODE = M_NOMINAL.COMPANYCODE AND D_DETAILS.NOMINAL = M_NOMINAL.NOMINAL INNER JOIN
                         M_COMPANY ON D_DETAILS.COMPANYCODE = M_COMPANY.COMPANYCODE LEFT OUTER JOIN
                         M_ACCOUNT ON D_DETAILS.ACCOUNT = M_ACCOUNT.ACCOUNT LEFT OUTER JOIN
                         M_LEDGER ON D_DETAILS.LEDGER = M_LEDGER.LEDGER

WHERE M_LEDGER.TYPE IN ('X','T') 
AND DOCTYPE != 'VAT'

AND D_DETAILS.ACCOUNT=@VATPeriod

AND D_DETAILS.COMPANYCODE=@CompanyCode

AND D_DETAILS.NOMINAL IN (@EUOUTPUTTAX) 

and doc_id in (
  
SELECT	DOC_ID
		 
FROM         M_USERFIELDS INNER JOIN
                      M_LEDGERUSERFIELDS INNER JOIN
                      M_ACCOUNT ON M_LEDGERUSERFIELDS.LEDGER_ID = M_ACCOUNT.LEDGER_ID ON M_USERFIELDS.ID = M_LEDGERUSERFIELDS.USERFIELD_ID INNER JOIN
                      D_DETAILS ON M_ACCOUNT.LEDGER = D_DETAILS.LEDGER AND M_ACCOUNT.ACCOUNT = D_DETAILS.ACCOUNT LEFT OUTER JOIN
                      D_ACCOUNTUSERFIELDS ON M_ACCOUNT.ID = D_ACCOUNTUSERFIELDS.ACCOUNT_ID AND M_USERFIELDS.ID = D_ACCOUNTUSERFIELDS.USERFIELD_ID

WHERE M_USERFIELDS.TITLE='EU Country'
	
AND d_details.LEDGER='SALES'

AND	 D_ACCOUNTUSERFIELDS.USERFIELD_VALUE='Yes'

AND	DOC_ID IN ( SELECT DOC_ID
FROM            D_DETAILS INNER JOIN
                         M_NOMINAL ON D_DETAILS.COMPANYCODE = M_NOMINAL.COMPANYCODE AND D_DETAILS.NOMINAL = M_NOMINAL.NOMINAL INNER JOIN
                         M_COMPANY ON D_DETAILS.COMPANYCODE = M_COMPANY.COMPANYCODE LEFT OUTER JOIN
                         M_ACCOUNT ON D_DETAILS.ACCOUNT = M_ACCOUNT.ACCOUNT AND D_DETAILS.LEDGER = M_ACCOUNT.LEDGER LEFT OUTER JOIN
                         M_LEDGER ON D_DETAILS.LEDGER = M_LEDGER.LEDGER

WHERE M_LEDGER.TYPE='X' AND D_DETAILS.DETAILSTATUS='O'

AND D_DETAILS.ACCOUNT=@VATPeriod

AND D_DETAILS.COMPANYCODE=@CompanyCode

AND D_DETAILS.NOMINAL IN (@EUOUTPUTTAX) ))</CommandText>
      </Query>
      <Fields>
        <Field Name="balance">
          <DataField>BALANCE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DOCDATE">
          <DataField>DOCDATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PERIOD">
          <DataField>PERIOD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCTYPE">
          <DataField>DOCTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCNUM">
          <DataField>DOCNUM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DETAILSTATUS">
          <DataField>DETAILSTATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
