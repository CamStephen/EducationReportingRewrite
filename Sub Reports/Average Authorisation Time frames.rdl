  <Code>Public Shared Function SetColor(ByVal Value As Integer) As String
SetColor = "Black"
If Value &lt; 0.00 Then 
SetColor = "Red"
End If
End Function

Public Function Divide(Var as double, Bud as double) AS double
    IF IsNothing(Var) Or IsNothing(Bud) Or Var = 0.00 Then
        Divide = 0
    ELSEIF Bud &lt;&gt; 0.00
                  Divide = (Var  / Bud) * 100
    ELSE
        Divide = 0
    END IF
End Function

Public Function ReplaceNull(VarValue as Double) AS Double

IF (IsNothing(VarValue)) 

                ReplaceNull = 0
ELSE 

                ReplaceNull = VarValue

END IF

End Function</Code>
